{
    "name": "mortogo321/laravel-notify",
    "description": "A Laravel package for sending notifications and alerts to multiple providers",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "notification",
        "alert",
        "slack",
        "discord",
        "telegram",
        "email",
        "sms"
    ],
    "authors": [
        {
            "name": "Mor",
            "email": "mortogo321@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Mortogo321\\LaravelNotify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mortogo321\\LaravelNotify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mortogo321\\LaravelNotify\\NotifyServiceProvider"
            ],
            "aliases": {
                "Notify": "Mortogo321\\LaravelNotify\\Facades\\Notify"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
